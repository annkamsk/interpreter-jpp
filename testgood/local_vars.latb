int fac(int a) {
	int r;
	int n;
	r = 1;
	n = a;
	while (n > 0) {
	  r = r * n;
	  n = n - 1;
	}
	return r;
}

string repStr(string s, int n) {
  string r = "";
  int i = 0;
  while(i<n) {
    r = r++s;
    i++;
  }
  return r;
}

